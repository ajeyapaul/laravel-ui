{
    "name": "ajeyapaul/laravel-ui",
    "description": "Basic UI starting point for Laravel",
    "keywords": [
        "ajeyapaul",
        "laravel-ui"
    ],
    "homepage": "https://github.com/ajeyapaul/laravel-ui",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ajeya Paul",
            "email": "paulajeya@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AjeyaPaul\\LaravelUi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AjeyaPaul\\LaravelUi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AjeyaPaul\\LaravelUi\\LaravelUiServiceProvider"
            ],
            "aliases": {
                "LaravelUi": "AjeyaPaul\\LaravelUi\\LaravelUiFacade"
            }
        }
    }
}
